CREATE TABLE london_listings (
    id BIGINT,
    name TEXT,
    host_id BIGINT,
    host_name TEXT,
    neighbourhood_group TEXT,
    neighbourhood TEXT,
    latitude TEXT,
    longitude TEXT,
    room_type TEXT,
    price REAL,
    minimum_nights INTEGER,
    number_of_reviews INTEGER,
    last_review DATE,
    reviews_per_month REAl,
    calculated_host_listings_count INTEGER,
    availability_365 TEXT
);

SELECT
	*
FROM 
	london_listings
ORDER BY price DESC;


SELECT
	COUNT(*),
	neighbourhood
FROM 
	london_listings
GROUP BY neighbourhood
ORDER BY COUNT DESC
